# Environment variables for Docker Compose
# Copy this file to .env and fill in your credentials

# Required: ZDF API OAuth2 Credentials
# Register at: https://developer.zdf.de/limited-access
ZDF_CLIENT_ID=your-client-id-here
ZDF_CLIENT_SECRET=your-client-secret-here

# Optional: Server Configuration
SERVER_PORT=8080
LOG_LEVEL=INFO

